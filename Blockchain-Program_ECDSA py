n\n","        genesis_hash = self.hash_block(\"genesis_block\")  # Membuat hash untuk blok genesis\n","        self.append_block(\n","            nonce=self.proof_of_work(0, genesis_hash, []),\n","            verifier=\"Genesis\",   # Verifier untuk blok genesis\n","            user=\"System\"         # Pengguna untuk blok genesis\n","        )\n","\n","    # Fungsi untuk melakukan proof-of-work pada suatu blok\n","    def proof_of_work(self, index, hash_of_previous_block, transactions):\n","        nonce = 0\n","        while self.valid_proof(index, hash_of_previous_block, transactions, nonce) is False:\n","            nonce += 1\n","        return nonce\n","\n","    # Fungsi untuk memeriksa validitas proof-of-work\n","    def valid_proof(self, index, hash_of_previous_block, transactions, nonce):\n","        content = f'({index}{hash_of_previous_block}{transactions}{nonce})'.encode()\n","        content_hash = hashlib.sha256(content).hexdigest()\n","        return content_hash[:len(self.difficulty_target)] == self.difficulty_target\n","\n","    # Fungsi untuk menambahkan blok baru ke rantai blockchain\n","    def append_block(self, nonce, verifier, user):\n","        block = {\n","            'index': len(self.chain),       # Nomor urut blok dalam rantai\n","            'timestamp': time(),            # Waktu ketika blok ditambang\n","            'transactions': self.current_transactions,  # Daftar transaksi dalam blok\n","            'nonce': nonce,                 # Nilai nonce yang dipilih untuk proof-of-work\n","            'hash_of_previous_block': self.hash_block(self.chain[-1]) if self.chain else None,  # Hash dari blok sebelumnya dalam rantai\n","            'verifier': verifier,           # Penambahan informasi verifikator\n","            'user': user                    # Penambahan informasi user\n","        }\n","        self.current_transactions = []     # Mengosongkan daftar transaksi yang belum ditambahkan\n","        self.chain.append(block)           # Menambahkan blok baru ke rantai\n","        return block\n","\n","    # Fungsi untuk menambahkan transaksi baru ke daftar transaksi yang belum ditambahkan\n","    def add_transaction(self, sender, recipient, amount):\n","        self.current_transactions.append({\n","            'amount': amount,               # Jumlah yang ditransfer dalam transaksi\n","            'recipient': recipient,         # Penerima dana\n","            'sender': sender                # Pengirim dana\n","        })\n","        return self.last_block['index'] + 1\n","\n","    # Fungsi property untuk mengakses blok terakhir dalam rantai\n","    @property\n","    def last_block(self):\n","        return self.chain[-1]\n","\n","    # Fungsi untuk memvalidasi unique ID\n","    def validate_unique_id(self, unique_id):\n","        return unique_id in self.unique_ids\n","\n","    # Fungsi untuk menghitung jumlah blok dalam blockchain\n","    def get_block_count(self):\n","        return len(self.chain)\n","\n","    # Fungsi untuk proof of authority (PoA)\n","    def proof_of_authority(self):\n","        authorities = [\"Perusahaan xyz\", \"Miner\"]  # Daftar otoritas\n","        return random.choice(authorities)  # Pilih otoritas secara acak\n","\n","    # Fungsi untuk mengeksekusi smart contract\n","    def execute_smart_contract(self, contract):\n","        # Implementasi sederhana hanya untuk konsep\n","        print(\"Smart Contract Executed:\", contract)\n","\n","    # Fungsi untuk membuat kunci privat dan publik\n","    def generate_keys(self):\n","        private_key = SigningKey.generate(curve=SECP256k1)\n","        public_key = private_key.get_verifying_key()\n","        return private_key, public_key\n","\n","    # Fungsi untuk menandatangani pesan\n","    def sign_message(self, private_key, message):\n","        signature = private_key.sign(message.encode())\n","        return signature\n","\n","    # Fungsi untuk memverifikasi tanda tangan\n","    def verify_signature(self, public_key, signature, message):\n","        try:\n","            public_key.verify(signature, message.encode())\n","            return True\n","        except:\n","            return False\n","\n","    # Fungsi untuk menambahkan fitur keamanan dengan ECDSA\n","    def security_features(self, data):\n","        private_key, public_key = self.generate_keys()\n","        signature = self.sign_message(private_key, data)\n","        verification_result = self.verify_signature(public_key, signature, data)\n","        return signature, verification_result\n","\n","# Program Utama\n","if __name__ == \"__main__\":\n","    start_time = None  # Waktu awal program dimulai\n","\n","    blockchain = Blockchain()  # Membuat instance kelas Blockchain\n","    node_identifier = str(uuid4()).replace('-', \"\")  # Menghasilkan identifikasi node\n","\n","    unique_id = input(\"Masukkan Unique ID: \")  # Meminta unique ID dari pengguna\n","\n","    if blockchain.validate_unique_id(unique_id):\n","        start_time = time()  # Memulai hitung waktu setelah validasi unique ID\n","\n","        # Menambahkan transaksi baru\n","        blockchain.add_transaction(sender=\"0\", recipient=node_identifier, amount=1)\n","        verifier = blockchain.proof_of_authority()  # Menggunakan PoA untuk memilih verifikator\n","        user = \"User\"                    # Pengguna adalah pengirim\n","        nonce = blockchain.proof_of_work(len(blockchain.chain), blockchain.hash_block(blockchain.last_block), blockchain.current_transactions)  # Melakukan proof-of-work\n","        block = blockchain.append_block(nonce, verifier, user)  # Menambahkan blok baru ke rantai\n","\n","        # Mencetak blok yang baru ditambang\n","        print(\"Blok baru telah ditambahkan (ditambang)\")\n","        print(\"Indeks:\", block['index'])\n","        print(\"Hash Blok Sebelumnya:\", block['hash_of_previous_block'])\n","        print(\"Nonce:\", block['nonce'])\n","        print(\"Transaksi:\", block['transactions'])\n","        print(\"Verifikator:\", block['verifier'])  # Menampilkan informasi verifikator\n","        print(\"User:\", block['user'])            # Menampilkan informasi user\n","\n","        # Output dari ECDSA\n","        data = \"Data yang akan ditandatangani\"\n","        signature, verification_result = blockchain.security_features(data)\n","        print(\"\\nOutput dari ECDSA:\")\n","        print(\"Data:\", data)\n","        print(\"Tanda Tangan:\", signature) #ttd transaksi\n","        print(\"Hasil Verifikasi:\", verification_result)\n","    else:\n","        print(\"Unique ID Tidak Valid!\")\n","\n","    if start_time is not None:\n","        end_time = time()  # Waktu akhir program\n","        print(\"\\nTotal Waktu Komputasi:\", end_time - start_time, \"detik\")  # Cetak waktu komputasi keseluruhan\n","\n","    # Cetak jumlah blok dalam blockchain\n","    print(\"Jumlah Blok dalam Blockchain:\", blockchain.get_block_count())\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"tqf16V6p48pn","outputId":"f5864728-ee0a-48d2-8d92-c073eedd0b7d","executionInfo":{"status":"ok","timestamp":1714300853638,"user_tz":-420,"elapsed":5916,"user":{"displayName":"DICKY AFRIADY","userId":"17179287844358253311"}}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Masukkan Unique ID: asdwAWS123456!@#$dsm\n","Blok baru telah ditambahkan (ditambang)\n","Indeks: 1\n","Hash Blok Sebelumnya: 8d088a87e1f9e039e00b374212b7a7af8a3d53e35109f3dc60209640fdd826f1\n","Nonce: 22388\n","Transaksi: [{'amount': 1, 'recipient': 'ca09953a47a24b04bb2983620a939053', 'sender': '0'}]\n","Verifikator: Perusahaan xyz\n","User: User\n","\n","Output dari ECDSA:\n","Data: Data yang akan ditandatangani\n","Tanda Tangan: b'\\xf8\\x8f\\xaf\\xce\\xab\\x95\\x8cE\\xc4\\xec\\x08\\xcd.4\\xcb\\x99\\xff\\xd6i\\x9d-N|\\x033\\x8dY\\x9c\\xb6c\\xb8\\xee\\xa9P\\x83\\x92\\xa8r\\x05\\xcd\\xcf\\xa8\\xb1\\xadA\\x8f\\xa8|\\xf9\\xe5P\\x01`\\xb25\\x80;\\xe8\\x1be\\x0c\\xeck|'\n",