"        block = {\n","            'index': len(self.chain),       # Nomor urut blok dalam rantai\n","            'timestamp': time(),            # Waktu ketika blok ditambang\n","            'transactions': self.current_transactions,  # Daftar transaksi dalam blok\n","            'nonce': nonce,                 # Nilai nonce yang dipilih untuk proof-of-work\n","            'hash_of_previous_block': self.hash_block(self.chain[-1]) if self.chain else None,  # Hash dari blok sebelumnya dalam rantai\n","            'verifier': verifier,\n","            'user': user\n","        }\n","        self.current_transactions = []\n","        self.chain.append(block)\n","        return block\n","\n","    # Fungsi untuk menambahkan transaksi baru ke daftar transaksi yang belum ditambahkan\n","    def add_transaction(self, sender, recipient, amount):\n","        self.current_transactions.append({\n","            'amount': amount,\n","            'recipient': recipient,\n","            'sender': sender\n","        })\n","        return self.last_block['index'] + 1\n","\n","\n","    @property\n","    def last_block(self):\n","        return self.chain[-1]\n","\n","\n","    def validate_unique_id(self, unique_id):\n","        return unique_id in self.unique_ids\n","\n","\n","    def get_block_count(self):\n","        return len(self.chain)\n","\n","    # Fungsi untuk proof of authority (PoA)\n","    def proof_of_authority(self):\n","        authorities = [\"Perusahaan xyz\", \"Miner\"]  # Daftar otoritas\n","        return random.choice(authorities)\n","\n","    # Fungsi untuk mengeksekusi smart contract\n","    def execute_smart_contract(self, contract):\n","        # Implementasi sederhana hanya untuk konsep\n","        print(\"Smart Contract Executed:\", contract)\n","\n","    # Fungsi untuk membuat kunci privat dan publik\n","    def generate_keys(self):\n","        private_key = SigningKey.generate(curve=SECP256k1)\n","        public_key = private_key.get_verifying_key()\n","        return private_key, public_key\n","\n","\n","    def sign_message(self, private_key, message):\n","        signature = private_key.sign(message.encode())\n","        return signature\n","\n","\n","    def verify_signature(self, public_key, signature, message):\n","        try:\n","            public_key.verify(signature, message.encode())\n","            return True\n","        except:\n","            return False\n","\n","\n","    def security_features(self, data):\n","        private_key, public_key = self.generate_keys()\n","        signature = self.sign_message(private_key, data)\n","        verification_result = self.verify_signature(public_key, signature, data)\n","        return signature, verification_result\n","\n","\n","    def compute_time(self, start_time):\n","        end_time = time()\n","        print(\"\\nWaktu Komputasi:\", end_time - start_time, \"detik\")\n","        print(\"-\" * 30)\n","\n","# Program Utama\n","if __name__ == \"__main__\":\n","    blockchain = Blockchain()\n","\n","    while True:\n","        unique_id = input(\"\\nMasukkan Unique ID atau 'stop' untuk berhenti: \")\n","\n","        if unique_id == 'stop':\n","            break\n","\n","        start_time = time()\n","\n","        if blockchain.validate_unique_id(unique_id):\n","\n","            blockchain.add_transaction(sender=\"0\", recipient=str(uuid4()).replace('-', \"\"), amount=1)\n","            verifier = blockchain.proof_of_authority()\n","            user = \"User\"\n","            nonce = blockchain.proof_of_work(len(blockchain.chain), blockchain.hash_block(blockchain.last_block), blockchain.current_transactions)\n","            block = blockchain.append_block(nonce, verifier, user)\n","\n","\n","            print(\"Blok baru telah ditambahkan (ditambang)\")\n","            print(\"Indeks:\", block['index'])\n","            print(\"Hash Blok Sebelumnya:\", block['hash_of_previous_block'])\n","            print(\"Nonce:\", block['nonce'])\n","            print(\"Transaksi:\", block['transactions'])\n","            print(\"Verifikator:\", block['verifier'])\n","            print(\"User:\", block['user'])\n","\n","\n","            data = \"Data yang akan ditandatangani\"\n","            signature, verification_result = blockchain.security_features(data)\n","            print(\"\\nOutput dari ECDSA:\")\n","            print(\"Data:\", data)\n","            print(\"Tanda Tangan:\", signature)\n","            print(\"Hasil Verifikasi:\", verification_result)\n","\n","\n","            blockchain.compute_time(start_time)\n","        else:\n","            print(\"Unique ID Tidak Valid!\")\n","\n","\n","\n","    print(\"\\nJumlah Node dalam Blockchain:\", blockchain.get_block_count())\n"],"metadata":{"id":"_c3nUfmGh4UV","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1714307776531,"user_tz":-420,"elapsed":3040262,"user":{"displayName":"DICKY AFRIADY","userId":"17179287844358253311"}},"outputId":"fabc0c9e-817a-460b-b428-6b4fb89a870c"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Masukkan Unique ID atau 'stop' untuk berhenti: Kh8lO`F9CrQ2bn})|oh\n","Blok baru telah ditambahkan (ditambang)\n","Indeks: 1\n","Hash Blok Sebelumnya: 65c71f533d313fe1a75dccfd8559fe86edcc05959fdcb8660834c4106ba0b07f\n","Nonce: 20365\n","Transaksi: [{'amount': 1, 'recipient': '9312624dd89040ec84473085f68809df', 'sender': '0'}]\n","Verifikator: Miner\n","User: User\n","\n","Output dari ECDSA:\n","Data: Data yang akan ditandatangani\n","Tanda Tangan: b'\\x0e\\xe3\\x06\\xd9cE5\\xc7q\\xfc\\xa2\\xcex\\xe9\\xcc\\xc7\\xe3\\xce-\\xa3$x\\x01R\\xb9\\xf9\\xe85.v\\xb8\\xb2\\xb2\\xf3\\x96\\xe7\\xc3-\\xc5\\xfd\\x15\\xcen\\x1c\\xcb\\xa5\\xae\\x01\\xf6\\x15H\\x84\\x95\\x9e\\xdf\\x1d\\xd4x\\x82\\xad\\xc4E\\xafj'\n","Hasil Verifikasi: True\n","\n","Waktu Komputasi: 0.09041833877563477 detik\n","------------------------------\n","\n","Masukkan Unique ID atau 'stop' untuk berhenti: Eoh{r}^Erly<F;F