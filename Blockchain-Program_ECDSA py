alse:\n","            nonce += 1\n","        return nonce\n","\n","    # Fungsi untuk memeriksa validitas proof-of-work\n","    def valid_proof(self, index, hash_of_previous_block, transactions, nonce):\n","        content = f'({index}{hash_of_previous_block}{transactions}{nonce})'.encode()\n","        content_hash = hashlib.sha256(content).hexdigest()\n","        return content_hash[:len(self.difficulty_target)] == self.difficulty_target\n","\n","    # Fungsi untuk menambahkan blok baru ke rantai blockchain\n","    def append_block(self, nonce, verifier, user):\n","        block = {\n","            'index': len(self.chain),       # Nomor urut blok dalam rantai\n","            'timestamp': time(),            # Waktu ketika blok ditambang\n","            'transactions': self.current_transactions,  # Daftar transaksi dalam blok\n","            'nonce': nonce,                 # Nilai nonce yang dipilih untuk proof-of-work\n","            'hash_of_previous_block': self.hash_block(self.chain[-1]) if self.chain else None,  # Hash dari blok sebelumnya dalam rantai\n","            'verifier': verifier,           # Penambahan informasi verifikator\n","            'user': user                    # Penambahan informasi user\n","        }\n","        self.current_transactions = []     # Mengosongkan daftar transaksi yang belum ditambahkan\n","        self.chain.append(block)           # Menambahkan blok baru ke rantai\n","        return block\n","\n","    # Fungsi untuk menambahkan transaksi baru ke daftar transaksi yang belum ditambahkan\n","    def add_transaction(self, sender, recipient, amount):\n","        self.current_transactions.append({\n","            'amount': amount,               # Jumlah yang ditransfer dalam transaksi\n","            'recipient': recipient,         # Penerima dana\n","            'sender': sender                # Pengirim dana\n","        })\n","        return self.last_block['index'] + 1\n","\n","    # Fungsi property untuk mengakses blok terakhir dalam rantai\n","    @property\n","    def last_block(self):\n","        return self.chain[-1]\n","\n","    # Fungsi untuk memvalidasi unique ID\n","    def validate_unique_id(self, unique_id):\n","        return unique_id in self.unique_ids\n","\n","    # Fungsi untuk menghitung jumlah blok dalam blockchain\n","    def get_block_count(self):\n","        return len(self.chain)\n","\n","    # Fungsi untuk proof of authority (PoA)\n","    def proof_of_authority(self):\n","        authorities = [\"Perusahaan xyz\", \"Miner\"]  # Daftar otoritas\n","        return random.choice(authorities)  # Pilih otoritas secara acak\n","\n","    # Fungsi untuk menambahkan identitas pengguna\n","    def add_identity(self, identity_id, public_key):\n","        self.identity_pool[identity_id] = public_key\n","\n","    # Fungsi untuk memverifikasi identitas pengguna\n","    def verify_identity(self, identity_id, signature, message):\n","        public_key = self.identity_pool.get(identity_id)\n","        if public_key:\n","            try:\n","                vk = VerifyingKey.from_string(bytes.fromhex(public_key), curve=SECP256k1)\n","                return vk.verify(bytes.fromhex(signature), message.encode())\n","            except:\n","                return False\n","        return False\n","\n","# Program Utama\n","if __name__ == \"__main__\":\n","    blockchain = Blockchain()  # Membuat instance kelas Blockchain\n","    node_identifier = str(uuid4()).replace('-', \"\")  # Menghasilkan identifikasi node\n","\n","    # Tambahkan identitas contoh\n","    sk = SigningKey.generate(curve=SECP256k1)\n","    vk = sk.get_verifying_key()\n","    identity_id = \"user1\"\n","    blockchain.add_identity(identity_id, vk.to_string().hex())\n","\n","    # Penginputan unique ID satu per satu\n","    while True:\n","        unique_id = input(\"Masukkan Unique ID (atau ketik 'stop' untuk berhenti): \")\n","        if unique_id.lower() == 'stop':\n","            break\n","\n","        start_time = time()  # Waktu awal program dimulai\n","        if blockchain.validate_unique_id(unique_id):\n","            # Menambahkan transaksi baru\n","            blockchain.add_transaction(sender=\"0\", recipient=node_identifier, amount=1)\n","            verifier = random.choice([\"Miner\", \"Perusahaan xyz\"])  # Verifikator dipilih secara acak antara \"Miner\" dan \"Perusahaan\"\n","            user = \"User\"                    # Pengguna adalah pengirim\n","            nonce = blockchain.proof_of_work(len(blockchain.chain), blockchain.hash_block(blockchain.last_block), blockchain.current_transactions)  # Melakukan proof-of-work\n","            block = blockchain.append_block(nonce, verifier, user)  # Menambahkan blok baru ke rantai\n","\n","            # Mencetak blok yang baru ditambang\n","            print(\"Blok baru telah ditambahkan (ditambang)\")\n","            print(\"Indeks:\", block['index'])\n","            print(\"Hash Blok Sebelumnya:\", block['hash_of_previous_block'])\n","            print(\"Nonce:\", block['nonce'])\n","            print(\"Transaksi:\", block['transactions'])\n","            print(\"Verifikator:\", block['verifier'])  # Menampilkan informasi verifikator\n","            print(\"User:\", block['user'])            # Menampilkan informasi user\n","        else:\n","            print(\"Unique ID Tidak Valid!\")\n","\n","        end_time = time()  # Waktu akhir program\n","        print(\"Total Waktu Komputasi:\", end_time - start_time, \"detik\")  # Cetak waktu komputasi keseluruhan\n","        print(\"-\" * 30)\n","\n","    # Verifikasi contoh identitas\n","    message = \"Test message\"\n","    signature = sk.sign(message.encode()).hex()\n","    verification_result = blockchain.verify_identity(identity_id, signature, message)\n","    print(f\"Verifikasi Identitas untuk '{message}' dengan ID '{identity_id}':\", verification_result)\n","\n","    # Cetak jumlah blok dalam blockchain\n","    print(\"Jumlah Blok dalam Blockchain:\", blockchain.get_block_count())\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"nBrjMMUzgpGf","executionInfo":{"status":"ok","timestamp":1722810669675,"user_tz":-420,"elapsed":51419,"user":{"displayName":"DICKY AFRIADY","userId":"17179287844358253311"}},"outputId":"151a9a48-8bd9-434c-c23a-7e54f76128a2"},"execution_count":3,"outputs":[{"output_type":"stream","name":"stdout","text":["Masukkan Unique ID (atau ketik 'stop' untuk berhenti): \"ND3|r@Urn}plCAR|9KL{\"\n","Unique ID Tidak Valid!\n","Total Waktu Komputasi: 8.463859558105469e-05 detik\n","------------------------------\n","Masukkan Unique ID (atau ketik 'stop' untuk berhenti): ND3|r@Urn}plCAR|9KL{\n","Unique ID Tidak Valid!\n","Total Waktu Komputasi: 8.511543273925781e-05 detik\n","------------------------------\n","Masukkan Unique ID (atau ketik 'stop' untuk berhenti): bN~>vp`PF5R1=MQA1?1*\n","Unique ID Tidak Valid!\n","Total Waktu Komputasi: 0.00010347366333007812 detik\n","------------------------------\n","Masukkan Unique ID (atau ketik 'stop' untuk berhenti): stop\n","Verifikasi Identitas untuk 'Test message' dengan ID 'user1': True\n","Jumlah Blok dalam Blockchain: 1\n"]}]}]}